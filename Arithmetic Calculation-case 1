#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

       int a,b;
      char opt;
    float add,sub,multi,div,mdiv;
    scanf("%d\n",&a);
    scanf("%d\n",&b);
    scanf("%c\n",&opt);
    switch(opt)
    {
        case '+':
            add=a+b;
            printf("Addition of two number is %.1f",add);
            break;
        case '-':
            sub=a-b;
            printf("Subtraction of two number is %.1f",sub);
            break;
        case '*':
            multi=a*b;
            printf("Multiplication of two number is %.1f",multi);
            break;
        case '/':
            div=a/b;
            printf("Division of two number is %.1f",div);
            break;
        case '%':
            mdiv=a%b;
            printf("Modulodivision of two number is %.1f",mdiv);
            break;
        default:
            printf("Invalid Input");
    }
          
    return 0;
}
